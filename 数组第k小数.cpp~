#include<iostream>

using namespace std;

class Number{
public:
  Number(int v = 0):value(v){}
  int value;

};


void sortNumbers(Number &p,int n){

  for(int i = 0;i<n-1;i++){
    for(int j = 1;j<i;j++){
      if(p[j]>p[i]){
	int temp = p[j].value;
	p[j].value = p[i].value;
	p[i].value = temp;
      }
    }
  }


  
}



