#include<iostream>

using namespace std;

class Rectangle{
  int len,wid;
public:
  Rectangle(int l,int w):len(l),wid(w){}
  friend int Area(const Rectangle &r);
  friend int Round(const Rectangle &r);
};

Rectangle::Area(const Rectangle& r){
  return (r.len*r.wid);
}

Rectangle::Round(const Rectangle& r){
  return 2*(r.len+r.wid);
}

int main(){

  

  return 0;
}
